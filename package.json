{
  "name": "claude-code-sdk-github-actions-example",
  "version": "1.0.0",
  "description": "GitHub ActionsでClaude Code SDKを使ったコード生成のサンプル",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "node dist/generate-refactored.js",
    "generate:legacy": "node dist/generate-unified.js",
    "generate:direct": "node dist/generate.js",
    "generate:official": "node dist/generate-official.js",
    "dev": "tsx src/generate-refactored.ts",
    "dev:legacy": "tsx src/generate-unified.ts",
    "dev:direct": "tsx src/generate.ts",
    "dev:official": "tsx src/generate-official.ts"
  },
  "keywords": [
    "claude",
    "ai",
    "code-generation",
    "github-actions",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.0",
    "@anthropic-ai/sdk": "^0.27.0",
    "@aws-sdk/client-bedrock-runtime": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}